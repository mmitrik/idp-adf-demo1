{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "idpdemofactory"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://idpdemo1storage.dfs.core.windows.net/"
		},
		"DICOM Service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://idpdemo1ws-idpdemo1dicom.dicom.azurehealthcareapis.com"
		},
		"DICOM Service_properties_typeProperties_aadResourceId": {
			"type": "string",
			"defaultValue": "https://dicom.healthcareapis.azure.com"
		},
		"hourly_properties_pipeline_parameters_StartTime": {
			"type": "string",
			"defaultValue": "@formatDateTime(trigger().outputs.windowStartTime)"
		},
		"hourly_properties_pipeline_parameters_EndTime": {
			"type": "string",
			"defaultValue": "@formatDateTime(trigger().outputs.windowEndTime)"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "idpdemofactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"DICOM Service_properties_typeProperties_url": {
						"value": "[parameters('DICOM Service_properties_typeProperties_url')]"
					},
					"DICOM Service_properties_typeProperties_aadResourceId": {
						"value": "[parameters('DICOM Service_properties_typeProperties_aadResourceId')]"
					},
					"hourly_properties_pipeline_parameters_StartTime": {
						"value": "[parameters('hourly_properties_pipeline_parameters_StartTime')]"
					},
					"hourly_properties_pipeline_parameters_EndTime": {
						"value": "[parameters('hourly_properties_pipeline_parameters_EndTime')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}